generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created-at")
  updatedAt DateTime @updatedAt @map("updated-at")

  email String @unique
  password String
  name String
}

model Emails {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created-at")
  updatedAt DateTime @updatedAt @map("updated-at")

  emailTo String @map("email-to")
  emailFrom String @map("email-from")
  title String
  content String
}